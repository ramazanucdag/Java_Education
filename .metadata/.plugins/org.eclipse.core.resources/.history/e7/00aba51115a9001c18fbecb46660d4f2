	HashMap b = new HashMap();
	b.put("Türkiye", "Ankara");
	b.put("Fransa", "Paris");
	b.put("Ýngiltere", "Londra");
	
	Set s = b.entrySet();
	Iterator i = (Iterator) s.iterator();
	
	while(i.hasNext()) {
		Map.Entry item = (Map.Entry) i.next();
		System.out.println(item.getKey() + " => " + item.getValue());
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
